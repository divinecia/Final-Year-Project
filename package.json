{
  "name": "househelp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "next dev --turbo",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "build:azure": "next build && next export",
    "build:fast": "next build --no-lint",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:fast": "tsc --noEmit --skipLibCheck",
    "typecheck:watch": "tsc --noEmit --watch",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:seed-services": "tsx src/scripts/seed-services-locations.ts",
    "db:seed-admin": "tsx src/scripts/seed-admin.ts",
    "clean": "rm -rf .next && rm -f tsconfig.tsbuildinfo",
    "clean:all": "npm run clean && rm -rf node_modules && npm install"
  },
  "dependencies": {
    "@genkit-ai/firebase": "^1.15.5",
